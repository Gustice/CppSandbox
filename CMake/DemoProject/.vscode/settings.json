{
    "clang.executable": "clang",
    "clang.completion.enable": true,
    "clang.cxxflags": [
        "-Wno-pragma-once-outside-header",
        "-std=c++11",
        "-I/${workspaceRoot}lib",
    ],
    "clang.cflags": [
        "-Wno-pragma-once-outside-header",
        "-std=c99",
        "-I/${workspaceRoot}lib",
    ],
    "files.associations": {
        "*.in": "cpp"
    },
}